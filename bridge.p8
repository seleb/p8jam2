pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
chain = {}
player = {}
cam = {}

gravity = 1

log_rad = 5
log_len = 32
gap = 8
gap2 = gap*gap
bridge_height = 64
damping = 0.75

function _init()
 local num_links = 128/gap
 for x=1,num_links do
  local link = {}
  link.x = x*gap
  link.y = bridge_height
  link.v = 0
  link.dv = 0
  if(x==1)then
   link.up = nil
  else
   link.up = chain[x-1]
   link.up.down = link
  end
  link.down = nil
  chain[x] = link
 end
 
 player.frame = 1
 player.grounded = false
 player.link1 = flr(#chain/2)
 player.link2 = player.link1+1
 player.x = chain[player.link1].x
 player.y = chain[player.link1].y
 player.vy = 0
 player.vx = 0
 player.dv = 0
 
 cam.x = 0
 cam.y = 0
end


function _update()
 --local speed = 15
 --local ctrl = flr(#chain/2)
 --if(btn(2)) chain[ctrl].dv -= speed
 --if(btn(3)) chain[ctrl].dv += speed

 for link in all(chain) do
  if(link.down) then
   local dy = link.down.y - link.y
   link.dv += dy*1.1
  end
  if(link.up) then
   local dy = link.up.y- link.y
   link.dv += dy*1.1
  end
 end
 
 -- apply gravity
 for link in all(chain) do
  link.dv += -gravity--+sin(time())
 end
 
 -- integrate
 for link in all(chain) do
  link.v += link.dv
  link.v *= damping
  link.y += link.v
  link.dv = 0
 end
 
 -- player
 player.dv = 0
 local ratio = (player.x%gap)/gap
 local links_y = chain[player.link1].y*(1-ratio) + chain[player.link2].y*ratio
 if(
  player.y+1 > links_y
  or
  player.y+player.vy+1 > links_y
 )then
  -- player pushes bridge
  if(player.vy > 0) then
   chain[player.link1].v += player.vy*(1-ratio)
   chain[player.link2].v += player.vy*(1-ratio)
  end
  player.dv -= 0.5*chain[player.link1].v*(1-ratio)
  player.dv -= 0.5*chain[player.link2].v*ratio
  player.grounded = true
  player.y = links_y
 else
  player.grounded = false
 end
 player.dv += gravity
 player.vy += player.dv
 player.y += player.vy
 
 --run
 if(btn(0)) then
  player.vx -= 1
 end
 if(btn(1)) then
  player.vx += 1
 end
 --jump
 if(player.grounded) then
  if(btnp(2) or btnp(4)) then
   player.vy = -10
   player.frame = 1
  end
 end
 
 player.vx *= damping
 player.x += player.vx
 player.link1 = flr(player.x/gap)
 player.link2 = player.link1+1
 
 
 chain[1].y=bridge_height
 chain[#chain].y=bridge_height
 chain[1].vy=0
 chain[#chain].vy=0
 
 
 
 -- camera
 cam.x += (player.x - 64 - cam.x)*0.1
 cam.y += (player.y - 64 - cam.y)*0.1
 camera(cam.x, cam.y)
 
 
 if(btnp(5)) then
  col_offset += 1
  pal()
  for x=0,15 do
   pal(x,(x+col_offset)%16,1)
  end
 end
end

col_offset = 0
-- sets the color to _c + offset
-- and returns the modified color
function color2(_c)
 local c = (_c+col_offset)%16
 color(c)
 return c
end

function draw_player()
 sspr(8+player.frame*16,0,16,16,player.x-8-log_len/2,player.y-16)
 pset(player.x-log_len/2, player.y, 0)
end

function _draw()
 cls()
color(7)
	rectfill(cam.x,cam.y,cam.x+128,cam.y+128)
	
	
	color(8)
 for i in all(chain) do
  if(i!=chain[1]) then
  line(i.up.x-log_len, i.up.y+log_rad, i.x-log_len, i.y+log_rad)
  end
 end
 for i=#chain,1,-1 do
  if(i == player.link1) then
   draw_player()
  end
  i = chain[i]
  color(2)
  rectfill(i.x,i.y,i.x-log_len,i.y+log_rad*2)
  circfill(i.x,i.y+log_rad,log_rad)
  circfill(i.x-log_len,i.y+log_rad,log_rad)
  
  color(7)
  line(i.x-2,i.y+1,i.x-log_len-1,i.y+1)
  color(8)
  line(i.x-2,i.y+log_rad*2-1,i.x-log_len-1,i.y+log_rad*2-1)
 end
 color(8)
 for i in all(chain) do
  circfill(i.x,i.y+log_rad,log_rad-1)
  if(i.up) then
  line(i.up.x, i.up.y+log_rad, i.x, i.y+log_rad)
  end
 end
 
 color(0)
 cursor(cam.x+1,cam.y+1)
 print(time())
end
